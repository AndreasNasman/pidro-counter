{
  "extends": [
    "react-app",
    "plugin:react/recommended",
    "eslint:all",
    "plugin:@typescript-eslint/all",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/no-magic-numbers": ["error", { "ignore": [1] }],
    "@typescript-eslint/no-type-alias": "off",
    "one-var": "off",
    "sort-imports": "off"
  }
}
